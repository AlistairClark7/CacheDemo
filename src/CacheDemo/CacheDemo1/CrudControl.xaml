<UserControl x:Class="CacheDemo1.CrudControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CacheDemo1"
             mc:Ignorable="d" Height="76.108" Width="696.059">
    <Grid>
        <TextBox x:Name="textBox" Height="23" Margin="0,14,532,0" TextWrapping="Wrap" Text="{Binding Key, FallbackValue=AwesomeKey}" VerticalAlignment="Top" HorizontalAlignment="Right" Width="120"/>
        <Label x:Name="label" Content="Key" HorizontalAlignment="Right" Margin="0,10,657,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="Awesomeness Level" Margin="0,11,409,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="115"/>
        <TextBox x:Name="textBox1" Height="23" Margin="0,14,284,0" TextWrapping="Wrap" Text="{Binding Level, FallbackValue=1}" VerticalAlignment="Top" HorizontalAlignment="Right" Width="120"/>
        <TextBox x:Name="textBox2" Height="23" Margin="0,14,16,0" TextWrapping="Wrap" Text="{Binding Something, FallbackValue=Oh Wow}" VerticalAlignment="Top" HorizontalAlignment="Right" Width="120"/>
        <Label x:Name="label2" Content="Something Awesome" Margin="0,11,141,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="122"/>
        <Button x:Name="getButton" Content="Get" Margin="0,42,256,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="75"/>
        <Button x:Name="createButton" Content="Create" Margin="0,42,176,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="75" Click="createButton_Click"/>
        <Button x:Name="updateButton" Content="Update" Margin="0,42,96,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="75"/>
        <Button x:Name="deleteButton" Content="Delete" Margin="0,42,16,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="75"/>

    </Grid>
</UserControl>
